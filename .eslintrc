{
    "extends": "airbnb-base",
    "env": {
        "es6": true
    },
    "globals": {
        "Framework": true
    },
    "rules": {
        "semi": [2, "never"],
        "no-console": "off",
        "linebreak-style": [0, "error", "windows"],
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
